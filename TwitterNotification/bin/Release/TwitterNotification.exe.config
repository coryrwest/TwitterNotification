<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
	<appSettings>
	   
	   <!--This app only supports a gmail SMTP server with Two-Factor Authentication disabled.
	   There is support for more providers planned in the future.-->
	   
	   <!-- Twitter homepage of person that you want updates from -->
	   <add key="URL" value="https://twitter.com/person" />
	   <!-- Subject of the daily email -->
	   <add key="emailSubject" value="Latest Tweets" />
	   <!-- Number of hours in the past you want Tweets for -->
	   <add key="hours" value="48" />
	   <!-- Email address you want email sent to -->
	   <add key="toEmail" value="you@gmail.com" />
	   <!-- Email address you want email sent from. This is also the email used for the SMTP connection -->
	   <add key="fromEmail" value="you@gmail.com" />
	   <!-- Password for the SMTP Connection -->
	   <add key="smtpPassword" value="password" />
	   <!-- Host for the  SMTP Connection -->
	   <add key="smtpHost" value="smtp.gmail.com" />
	   <!-- Port for the SMTP Connection -->
	   <add key="smtpPort" value="587" />
	</appSettings>
</configuration>